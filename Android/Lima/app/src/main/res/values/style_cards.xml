<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Outline">
        <item name="rippleColor">@color/lightGray</item>
        <item name="cardElevation">0dp</item>
    </style>

    <style name="Outline.Black">
        <item name="strokeColor">@color/black</item>
    </style>

    <style name="Outline.PrimaryDark">
        <item name="strokeColor">@color/primaryDarkColor</item>
    </style>

    <style name="Outline.Black.Width_05">
        <item name="strokeWidth">0.5dp</item>
    </style>

    <style name="Outline.PrimaryDark.Width_1">
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="Outline.Black.Width_05.Radius6">
        <item name="cardCornerRadius">6dp</item>
    </style>

    <style name="Outline.PrimaryDark.Width_1.Radius10">
        <item name="cardCornerRadius">10dp</item>
    </style>

    <style name="Outline.PrimaryDark.Width_1.Radius10.MW">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Outline.Black.Width_05.Radius6.WW">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
</resources>
