<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/tasksFragment">

    <fragment
        android:id="@+id/tasksFragment"
        android:name="start.up.tracker.ui.tasks.TasksFragment"
        android:label="Home"
        tools:layout="@layout/fragment_tasks" >
        <action
            android:id="@+id/action_tasksFragment_to_addEditTaskFragment"
            app:destination="@id/addEditTaskFragment" />
    </fragment>
    <fragment
        android:id="@+id/addEditTaskFragment"
        android:name="start.up.tracker.ui.addedittask.AddEditTaskFragment"
        android:label="{title}"
        tools:layout="@layout/fragment_add_edit_task" >
        <argument
            android:name="task"
            app:argType="start.up.tracker.data.db.Task"
            app:nullable="true"
            android:defaultValue="@null" />
        <argument
            android:name="title"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/analyticsFragment"
        android:name="start.up.tracker.ui.analytics.AnalyticsFragment"
        android:label="Analytics"
        tools:layout="@layout/fragment_analytics" />
    <fragment
        android:id="@+id/categoryFragment"
        android:name="start.up.tracker.ui.categories.CategoriesFragment"
        android:label="Categories"
        tools:layout="@layout/fragment_categories" >
        <action
            android:id="@+id/action_categoryFragment_to_categoryInsideFragment"
            app:destination="@id/categoryInsideFragment" />
    </fragment>
    <fragment
        android:id="@+id/settingsFragment"
        android:name="start.up.tracker.ui.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <dialog
        android:id="@+id/deleteAllCompletedDialogFragment"
        android:name="start.up.tracker.ui.deleteallcompleted.DeleteAllCompletedDialogFragment"
        android:label="DeleteAllCompletedDialogFragment" /><action android:id="@+id/action_global_deleteAllCompletedDialogFragment" app:destination="@id/deleteAllCompletedDialogFragment"/>
    <fragment
        android:id="@+id/categoryInsideFragment"
        android:name="start.up.tracker.ui.categoryInside.CategoryInsideFragment"
        android:label="{titleCategory}"
        tools:layout="@layout/fragment_category_inside" >
        <argument
            android:name="category"
            app:argType="start.up.tracker.data.db.models.Category" />
        <argument
            android:name="titleCategory"
            app:argType="string" />
    </fragment>
</navigation>